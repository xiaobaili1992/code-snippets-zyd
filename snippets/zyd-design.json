        "\t{\r",
          "\t\tlabel: '续约1',\r",
          "\t\tkey: 'operate1',\r",
          "\t\tattrs: {}, // element-ui button按钮的属性\r",
          "\t\tevents: {\r",
            "\t\t\t// element-ui button按钮的方法",
            "\t\t\tclick(daSource, e) {\r",
              "\t\t\t\tconsole.error('daSource, e', daSource, e);\r",
            "\t\t\t},\r",
          "\t\t},\r",
        "\t},\r",
        "\t{\r",
          "\t\tlabel: '续约2',\r",
          "\t\tkey: 'operate2',\r",
          "\t\tattrs: {},\r",
          "\t\tevents: {\r",
            "\t\t\tclick(daSource, e) {\r",
              "\t\t\t\tconsole.error('daSource, e', daSource, e);\r",
            "\t\t\t},\r",
          "\t\t},\r",
