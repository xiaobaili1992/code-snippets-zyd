{
  "ZydLargeScreenScale": {
    "prefix": "zlss",
    "body": [
      "<ZydLargeScreenScale width=\"${1}\" height=\"${2}\">${3}</ZydLargeScreenScale>",
      "${4}"
    ],
    "description": "zyd large screen scale"
  },
  "ZydLongTextTooltip": {
    "prefix": "zltt",
    "body": [
      "<ZydLongTextTooltip text=\"${1}\" />",
      "${2}"
    ],
    "description": "zyd long text tooltip"
  },
  "ZydOperateBtn": {
    "prefix": "zob",
    "body": [
      "<ZydOperateBtn :operateBtnConfig=\"${1}\" :partitionNum=\"${2}\" :dataSource=\"${3}\" />",
      "${4}"
    ],
    "description": "zyd operate btn"
  },
