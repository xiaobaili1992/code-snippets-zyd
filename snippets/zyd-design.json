  "ZydTag ": {
    "prefix": "zt",
    "body": [
      "<ZydTag color=\"${1}\">${2}</ZydTag>",
      "${3}"
    ],
    "description": "zyd tag"
  },
  "ZydSplitPane": {
    "prefix": "zsp",
    "body": [
      "<ZydSplitPane>\r",
        "\t<template slot=\"left\">\r",
          "\t\t<div class=\"left\">${1}</div>\r",
        "\t</template>\r",
        "\t<template slot=\"right\">\r",
          "\t\t<div class=\"right\">${2}</div>\r",
        "\t</template>\r",
      "</ZydSplitPane>\r"
    ],
    "description": "zyd split pane"
  }
}