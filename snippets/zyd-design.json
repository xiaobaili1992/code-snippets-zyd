        "\t{\r",
          "\t\tlabel: '续约3',\r",
          "\t\tkey: 'operate3',\r",
          "\t\tattrs: {},\r",
          "\t\tevents: {\r",
            "\t\t\tclick(daSource, e) {\r",
              "\t\t\t\tconsole.error('daSource, e', daSource, e);\r",
            "\t\t\t},\r",
          "\t\t},\r",
        "\t},\r",
